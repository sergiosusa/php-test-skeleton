    "name": "php-test-skeleton",
    "description": "Empty php skeleton ready to use, include infrastructure via installed using docker-compose.",
    "require": {
        "php": "^7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
